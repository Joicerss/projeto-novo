# Exemplo de Configuração de Variáveis de Ambiente
# ================================================
# Copie este arquivo para .env e configure suas credenciais

# API DataJud/CNJ (para integração futura)
DATAJUD_API_URL=https://api.datajud.cnj.jus.br
DATAJUD_API_KEY=sua-chave-api-aqui
DATAJUD_CLIENT_ID=seu-client-id
DATAJUD_CLIENT_SECRET=seu-client-secret

# Configurações de coleta
DATAJUD_TIMEOUT=30
DATAJUD_MAX_RETRIES=3
DATAJUD_RATE_LIMIT=100  # requisições por minuto

# Configurações do projeto
LOG_LEVEL=INFO
DEBUG_MODE=False

# Diretórios
DATA_DIR=data
OUTPUT_DIR=output
NOTEBOOKS_DIR=notebooks

# Jupyter Notebook
JUPYTER_PORT=8888
JUPYTER_TOKEN=  # deixe vazio para desabilitar token
